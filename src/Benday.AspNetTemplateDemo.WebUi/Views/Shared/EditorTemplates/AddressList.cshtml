@model List<Benday.AspNetTemplateDemo.Api.Address>

@{
    var addressForLabels = new Address();
}

<div class="row">
    <div class="col-2">@Html.DisplayNameFor(model => addressForLabels.Type)</div>
    <div class="col-2">@Html.DisplayNameFor(model => addressForLabels.Line1)</div>
    <div class="col-2">@Html.DisplayNameFor(model => addressForLabels.Line2)</div>
    <div class="col-2">@Html.DisplayNameFor(model => addressForLabels.City)</div>
    <div class="col-2">@Html.DisplayNameFor(model => addressForLabels.State)</div>
    <div class="col-2">@Html.DisplayNameFor(model => addressForLabels.PostalCode)</div>
</div>
@foreach (var item in Model)
{
    <div class="row">

        @Html.EditorFor(model => item)
    </div>
}
